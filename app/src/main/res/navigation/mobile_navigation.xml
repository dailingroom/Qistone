<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.qistone.ui.home"
        android:label="主页"
        tools:layout="@layout/home_fragment" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.qistone.ui.findFragment"
        android:label="发现"
        tools:layout="@layout/fragment_find" />

    <fragment
        android:id="@+id/navigation_msg"
        android:name="com.example.qistone.ui.msgFragment"
        android:label="消息"
        tools:layout="@layout/fragment_msg" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.qistone.ui.mineFragment"
        android:label="我的"
        tools:layout="@layout/fragment_mine" />
</navigation>